{
  "name": "hidkeyboard",
  "description": "Send USB HID keypresses from Micro:bit V2 (based on pxt bleHID)",
  "version": "1.0.0",
  "dependencies": {
    "core": "*"
  },
  "files": ["main.ts", "shims.d.ts", "test.ts"],
  "native": true,
  "yotta": {
    "config": {
      "microbit-dal": {
        "bluetooth": {
          "enabled": 0
        },
        "usb": {
          "enabled": 1
        }
      }
    },
    "dependencies": {
      "microbit-dal": "lancaster-university/microbit-dal#v2.2.0-rc9"
    }
  },
  "testFiles": ["test.ts"],
  "public": true,
  "license": "MIT"
}